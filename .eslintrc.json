{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "linebreak-style": [
      "error",
      "windows"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/destructuring-assignment": 0,
    "arrow-body-style": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "no-unused-expressions": 0,
    "operator-linebreak": 0,
    "import/extensions": 0,
    "indent": "off",
    "semi": 0,
    "no-undef": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": 0,
    "no-unused-vars": 0,
    "prefer-const": 0,
    "object-curly-newline": 0,
    "consistent-return": 0,
    "no-useless-escape": 0,
    "no-control-regex": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-prototype-builtins": 0
  }
}
